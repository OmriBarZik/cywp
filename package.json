{
  "name": "cypress-for-wordpress",
  "version": "0.0.0-development",
  "description": "cypress environment builder for wordpress",
  "author": "Omri BarZik <omri@bar-zik.com>",
  "main": "index.js",
  "licenses": [
    {
      "type": "GPLv2",
      "url": "https://www.gnu.org/licenses/old-licenses/gpl-2.0.txt"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OmriBarZik/cywp.git"
  },
  "bugs": {
    "url": "https://github.com/OmriBarZik/cywp/issues"
  },
  "files": [
    "src/**",
    "bin/cywp.js"
  ],
  "directories": {
    "lib": "./src ",
    "test": "./test",
    "bin": "./bin"
  },
  "scripts": {
    "start": "node index",
    "test": "jest",
    "snyk": "snyk test --dev",
    "coverage": "jest --coverage",
    "coverage:ci": "npm run coverage && codecov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-push": "run-p coverage lint snyk",
    "prepare": "husky install",
    "docs": "node bin/preperdocs.js",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "cypress": ">=6.7.x"
  },
  "dependencies": {
    "command-exists": "1.2.9",
    "commander": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "codecov": "^3.7.2",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsdoc": "^30.6.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-spellcheck": "^0.0.17",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^6.0.0",
    "jest": "^26.5.2",
    "jsdoc-to-markdown": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^17.4.4",
    "snyk": "^1.563.0"
  },
  "keywords": [
    "wordpress",
    "cypress",
    "cywp",
    "e2e",
    "test"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "./src/**"
    ],
    "testTimeout": 20000
  }
}
