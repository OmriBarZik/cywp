{
  "name": "cypress-for-wordpress",
  "version": "0.0.0-development",
  "description": "cypress environment builder for wordpress",
  "author": "Omri BarZik <omri@bar-zik.com>",
  "main": "index.js",
  "licenses": [
    {
      "type": "GPLv2",
      "url": "https://www.gnu.org/licenses/old-licenses/gpl-2.0.txt"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OmriBarZik/cywp.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/OmriBarZik/cywp/issues"
  },
  "files": [
    "src/**",
    "bin/cywp.js"
  ],
  "directories": {
    "lib": "./src ",
    "test": "./test"
  },
  "bin": {
    "cywp": "bin/cywp.js"
  },
  "scripts": {
    "start": "node index",
    "test": "jest",
    "snyk": "snyk test",
    "coverage": "jest --coverage",
    "coverage:ci": "npm run coverage && codecov",
    "lint": "concurrently 'npm:lint:*(!fix)'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:fix": "concurrently 'npm:lint:fix:*'",
    "lint:fix:eslint": "eslint . --fix",
    "lint:fix:prettier": "prettier --write .",
    "pre-push": "concurrently -k npm:coverage npm:lint npm:snyk",
    "prepare": "husky install",
    "docs": "node bin/preperdocs.js"
  },
  "peerDependencies": {
    "cypress": "^6.7.x || ^7.x.x || ^8.x.x || ^9.x.x"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || ^16.0.0"
  },
  "dependencies": {
    "command-exists": "1.2.9",
    "commander": "8.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/jest": "27.0.1",
    "codecov": "3.8.3",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jsdoc": "^38.1.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-spellcheck": "0.0.19",
    "husky": "^8.0.1",
    "jest": "27.2.0",
    "jsdoc-to-markdown": "7.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.6.2",
    "snyk": "^1.1039.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "wordpress",
    "cypress",
    "cywp",
    "e2e",
    "test"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "./src/**"
    ],
    "testTimeout": 20000
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,md,yml,json}": "prettier --write"
  }
}
