{
  "name": "cypress-for-wordpress",
  "version": "0.1.0",
  "description": "cypress environment builder for wordpress",
  "author": "Omri BarZik <omri@bar-zik.com>",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OmriBarZik/cywp.git"
  },
  "bugs": {
    "url": "https://github.com/OmriBarZik/cywp/issues"
  },
  "directories": {
    "lib": "./src ",
    "test": "./test",
    "bin": "./bin"
  },
  "scripts": {
    "start": "node index",
    "test": "jest",
    "coverage": "jest --coverage",
    "coverage:ci": "npm run coverage && codecov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-push": "run-p coverage lint",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "cypress": ">=6.7.x"
  },
  "dependencies": {
    "command-exists": "1.2.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "codecov": "^3.7.2",
    "eslint": "7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsdoc": "^30.6.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-spellcheck": "0.0.17",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^6.0.0",
    "jest": "^26.5.2",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "wordpress",
    "cypress",
    "cywp",
    "e2e",
    "test"
  ],
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "./src/**"
    ],
    "testTimeout": 20000
  }
}
