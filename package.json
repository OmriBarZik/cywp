{
  "name": "cypress-for-wordpress",
  "version": "0.1.0",
  "description": "cypress environment builder for wordpress",
  "main": "index.js",
  "author": "Omri BarZik <omri@bar-zik.com>",
  "license": "MIT",
  "scripts": {
    "start": "node index",
    "test": "mocha --recursive",
    "coverage": "nyc --reporter=lcov npm run test && codecov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "command-exists": "1.2.9",
    "dockerode": "^3.2.1"
  },
  "bin": {
    "cywp": "./bin/cywp.js"
  },
  "devDependencies": {
    "codecov": "^3.7.2",
    "eslint": "7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js"
    ],
    "package": "./package.json",
    "reporter": "spec",
    "slow": 75,
    "timeout": 2000,
    "ui": "bdd",
    "watch-files": [
      "lib/**/*.js",
      "test/**/*.js"
    ],
    "watch-ignore": [
      "lib/vendor"
    ]
  }
}
