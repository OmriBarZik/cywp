{
  "name": "cypress-for-wordpress",
  "version": "0.0.0-development",
  "description": "cypress environment builder for wordpress",
  "author": "Omri BarZik <omri@bar-zik.com>",
  "main": "index.js",
  "licenses": [
    {
      "type": "GPLv2",
      "url": "https://www.gnu.org/licenses/old-licenses/gpl-2.0.txt"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OmriBarZik/cywp.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/OmriBarZik/cywp/issues"
  },
  "files": [
    "src/**",
    "bin/cywp.js"
  ],
  "directories": {
    "lib": "./src ",
    "test": "./test"
  },
  "bin": {
    "cywp": "bin/cywp.js"
  },
  "scripts": {
    "start": "node index",
    "test": "jest",
    "snyk": "snyk test",
    "coverage": "jest --coverage",
    "coverage:ci": "npm run coverage && codecov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-push": "run-p coverage lint snyk",
    "prepare": "husky install",
    "docs": "node bin/preperdocs.js",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "cypress": ">=6.7.x"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "command-exists": "1.2.9",
    "commander": "8.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/jest": "27.0.1",
    "codecov": "3.8.3",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-jsdoc": "36.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-spellcheck": "0.0.19",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "jsdoc-to-markdown": "7.0.1",
    "lint-staged": "^11.2.6",
    "npm-run-all": "4.1.5",
    "semantic-release": "17.4.7",
    "snyk": "^1.744.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "wordpress",
    "cypress",
    "cywp",
    "e2e",
    "test"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "./src/**"
    ],
    "testTimeout": 20000
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
